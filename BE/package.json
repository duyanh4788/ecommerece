{
    "name": "ecommerce",
    "version": "1.0.0",
    "description": "API SOCKET built with NodeJS, Express, MySql and TypeScript",
    "main": "src/server.ts",
    "private": true,
    "scripts": {
        "test": "jest --runInBand",
        "test:watch": "jest --runInBand --watchAll",
        "build": "tsc",
        "env-dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "dev": "node -r ./node_modules/.bin/ts-node-dev --respawn --transpile-only src/server.ts",
        "start": "node build/server.js",
        "watch-ts": "tsc -w",
        "prod": "npm run build && npm run start",
        "eject": "nodejs-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "dependencies": {
        "@aws-sdk/lib-storage": "3.293.0",
        "@ffmpeg-installer/ffmpeg": "1.1.0",
        "@types/bcryptjs": "2.4.2",
        "@types/cookie-parser": "1.4.3",
        "@types/express": "4.17.13",
        "@types/express-session": "1.17.6",
        "@types/fluent-ffmpeg": "2.1.21",
        "@types/jsonwebtoken": "8.5.8",
        "@types/morgan": "1.9.4",
        "@types/mysql": "2.15.21",
        "@types/nodemailer": "6.4.7",
        "@types/socket.io": "3.0.2",
        "@types/winston": "2.4.4",
        "authenticator": "1.1.5",
        "axios": "1.3.4",
        "bad-words": "^3.0.4",
        "bcryptjs": "2.4.3",
        "cookie-parser": "1.4.6",
        "debug": "4.3.4",
        "dotenv": "^10.0.0",
        "engine.io": "6.2.0",
        "express": "4.18.1",
        "express-session": "1.17.3",
        "fluent-ffmpeg": "2.1.2",
        "jsonwebtoken": "^8.5.1",
        "morgan": "1.10.0",
        "multer": "1.4.5-lts.1",
        "mysql": "2.18.1",
        "mysql2": "3.4.0",
        "nodemailer": "6.9.1",
        "redis": "4.6.5",
        "sequelize": "6.32.1",
        "sequelize-typescript": "2.1.5",
        "sharp": "0.31.3",
        "socket.io": "^4.6.2",
        "ts-node-dev": "2.0.0",
        "winston": "3.8.2"
    },
    "engines": {
        "node": ">=14"
    },
    "devDependencies": {
        "@types/authenticator": "1.1.1",
        "@types/bad-words": "^3.0.1",
        "@types/jest": "29.5.1",
        "@types/multer": "1.4.5",
        "@types/sharp": "0.31.1",
        "@typescript-eslint/eslint-plugin": "5.54.1",
        "@typescript-eslint/parser": "5.54.1",
        "eslint": "8.36.0",
        "eslint-config-prettier": "8.7.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "29.5.0",
        "prettier": "2.8.4",
        "ts-jest": "29.1.0",
        "typescript": "4.9.5"
    }
}
